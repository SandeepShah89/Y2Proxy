         var $ = root.optimizely.$,\n                    counter = 0,\n                    defer = $.Deferred(),\n                    delay = setInterval(function() {\n\n                        ++counter;\n                        $.each(testSelectors, function(i, selector) {\n                            if ((typeof selector === 'function' && $(selector()).length) ||\n                                (typeof selector !== 'function' && $(selector).length)) {\n                                methods.log('Option ' + i + ' found ready after ' + (counter * 60 / 1000) + 's');\n                                clearInterval(delay);\n                                defer.resolve();\n                                return false;\n                            } else if (counter > 250) {\n\n                                clearInterval(delay);\n                                defer.reject();\n                                return false;\n                            }\n                        });\n                    }, 60);\n                return defer.promise();\n            },\n            /*\n                waitForDOMNodes - \n                @selectors - ['.class','#id'] \n                \n                returns Promise\n\n                waits for all selectros to be found or \n                if selectors is function, function result to be == true\n            */\n        //wait until all found\n        //or wait until finish and then \n            waitForDOMNodes: function(selectors, options) {\n                options = options || {};\n                var alwaysResolve = options.alwaysResolve || false;\n\n                if(!selectors || typeof selectors==='string') {\n                    methods.log('project.js waitForDOMNodes() is called with wrong arguments');\n                    methods.log(selectors);\n                    return;\n                }\n\n                var $ = root.optimizely.$;\n                var defer = $.Deferred();\n                \n                var collectItems = function(){\n                    var collected = {};\n                    for(var i = 0; i < selectors.length; i++){\n                        var $el = $(selectors[i]);\n\n                        if($el.length > 0) {\n                            collected[selectors[i]] = $el;\n                        }\n                    }\n                    return collected;\n                };\n                var lastCollected;\n                methods.waitUntilPass(function(c){\n                    \n                    lastCollected = collectItems();\n                    if(Object.keys(lastCollected).length != selectors.length) return;\n\n                    c(lastCollected);\n                })\n                .then(defer.resolve)\n                .fail(function(){\n                    if(alwaysResolve){\n                        defer.resolve(lastCollected);\n                    }else{\n                        defer.reject();\n                    }\n                });\n\n                return defer.promise();\n            },\n            /*\n                waitUntilPass\n                @conditionFunction - function()\n                @maxWaitTime(optional) - ms int - default 10s\n\n                waits until conditionFunction(c) calls c()\n                the reason insteaf of waiting for true is that sometimes \n                there is need to get result from conditional activation f\n            */\n            waitUntilPass: function(conditionFunction, maxWaitTime){\n                var $ = root.optimizely.$;\n                var defer = $.Deferred();\n                var intervalTime = 75;\n                maxWaitTime = maxWaitTime || 10 * 1000; //10 sec.\n                var totalChecks = 0;\n\n                var waitTimer = setInterval(function(){\n                    totalChecks++;\n                    if(maxWaitTime < intervalTime * totalChecks) {\n                        clearInterval(waitTimer);\n                        defer.reject();\n                    }else{\n                        conditionFunction(function(result){\n                            clearInterval(waitTimer);\n                            defer.resolve(result);\n                        });\n                    }\n                }, 60);\n\n                return defer.promise();\n            },\n            \n\n            //invokes debugger; if query has optimizely_debugger set\n            setBreakPoint: function(){\n                var s = {};\n\n                root.location.search\n                .substring(1) //remove ? char\n                .split('&') \n                .forEach(function (query){ \n                    query = query.split('='); \n                    s[query[0]] = query[1]; \n                });\n\n                if( s['optimizely_debugger'] ) {\n                    debugger;\n                }\n            },\n            synchronousDelay: function(secs) {\n                var start = +new Date();\n                do {} while (+new Date() < start + secs * 1000);\n            },\n            requestCallback: function(delay, async, callback, id) {\n                var $ = root.optimizely.$,\n                    experiment_data = this.setExperimentData(id),\n                    start = +new Date();\n                if (async === undefined) {\n                    async = true;\n                }\n                if (delay === undefined) {\n                    delay = 0;\n                }\n                console.log('Request', experiment_data.respondServerUrl + '?d=' + 1000 * delay);\n                $.ajax({\n                    async: false,\n                    url: experiment_data.respondServerUrl + '?d=' + 1000 * delay,\n                    complete: function( /*response, status*/ ) {\n                        if (typeof callback === 'function') {\n                            callback(+new Date() - start);\n                        }\n                    }\n                });\n            },\n   