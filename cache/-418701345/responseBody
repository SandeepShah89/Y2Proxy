	var checkTimes = 0;
	var maxCheckTimes = 50;
	//don't activate if it is not article (gallery article page uses .page-gallery-article class)
	

	var checkInterval = setInterval(function() {


	
	
		if(checkTimes > maxCheckTimes) {
			clearInterval(checkInterval);
		}
		
		checkTimes++;

		var pageType = $('meta[name="tmgads.pagetype"]').eq(0).attr('content');
		if(['story'].indexOf(pageType) > -1) {

			//don't activate for sponsored pages
			var metas = $('meta[name="DCSext.sponsored"], meta[name="tmg.sponsor"]');
			for(var i=0; i < metas.length; i++){
				if(metas[i].content) {
					return;
				}
			}
			//don't activate for business sponsored & commerce
			var businessMetas = $('meta[name="tmgads.businessSegment"]');
			if (businessMetas.length) {
				for(var i=0; i < businessMetas.length; i++){
					if(businessMetas[i].content === 'sponsored') {
						return;
					}
					if(businessMetas[i].content === 'commerce') {
						return;
					}
				}
			}

			if(dataLayer.premiumContent){
				return;
			}


			clearInterval(checkInterval); 
			//passed to here - let's activate
			activate();
		}
	}, 100);
