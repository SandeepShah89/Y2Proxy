 defer.promise();\n            },\n            adBlockTypeDetectionPromise: function() {\n                var $ = root.optimizely.$,\n                    isIOS = /iPad|iPhone|iPod/i.test(navigator.platform),\n                    responses = {},\n                    defer = $.Deferred(),\n                    assess = function(call, status) {\n                        responses[call] = status;\n                        if (\n                            (responses.safe && isIOS) &&\n                            (responses.ghostery === 0 || responses.both === 0 || responses.plus === 0)\n                        ) {\n                            defer.resolve('IOS');\n                        }\n                        if (responses.ghostery === 0) {\n                            defer.resolve('Ghostery');\n                        }\n                        if (responses.safe && responses.both && responses.ghostery) {\n                            defer.resolve('noAdBlockerOrUndetected');\n                        }\n                        if (responses.safe && responses.both === 0 && responses.plus) {\n                            defer.resolve('AB');\n                        } else if (responses.safe && responses.both === 0 && responses.plus === 0) {\n                            defer.resolve('ABP');\n                        }\n                    };\n                $.ajax({\n                        type: 'head',\n                        url: 'http://meraxes-cdn.polarmobile.com/mrm/v1.0.0/json/site/8147951252164934bcac37f1280cbe8a'\n                    })\n                    .complete(function(r) {\n\n                        assess('ghostery', r.status);\n                    });\n                $.get('http://optimize.aws.telegraph.co.uk:8001/safe').complete(function(r) {\n                    assess('safe', r.status);\n                });\n                $.get('http://optimize.aws.telegraph.co.uk:8001/securepubads.g.doubleclick.net').complete(\n                    function(r) {\n                        assess('both', r.status);\n                    });\n                $.get('http://optimize.aws.telegraph.co.uk:8001/pixel.quantserve.com').complete(function(r) {\n                    assess('plus', r.status);\n                });\n                return defer.promise();\n            },\n            getUserStatus: function(id) {\n\n                // add id argument to request to have status returned to\n                // experiment's data object\n                // if only reuirement is to fire events then this is not required\n                var experiment_data = {};\n                if (id) {\n                    experiment_data = methods.getExperimentData(id);\n                }\n\n                // when logged in\n                experiment_data.tmg_pid = methods.readCookie('tmg_pid');\n                experiment_data.logged_in = typeof experiment_data.tmg_pid !==\n                    'object' &&\n                    experiment_data.tmg_pid ? true : false;\n\n                // info on subscriber/registered user \n                experiment_data.tmg_p13n = methods.readCookie('tmg_p13n');\n                experiment_data.subscriber = typeof experiment_data.tmg_p13n !==\n                    'object' ?\n                    experiment_data.tmg_p13n\n                    .replace(/^.*?subscriber:'([^']*?)'.*?$/, function($0, $1) {\n                        return $1;\n                    }) : 'false';\n                experiment_data.subscriber = experiment_data.subscriber ===\n                    'true' ? true : false;\n                experiment_data.registered = typeof experiment_data.tmg_p13n !==\n                    'object' ?\n                    experiment_data.tmg_p13n\n                    .replace(/^.*?subscriber:'([^']*?)'.*?$/, function($0, $1) {\n                        return $1 === 'false' ? 'true' : 'false';\n                    }) : 'false';\n                experiment_data.registered = experiment_data.registered === 'tru