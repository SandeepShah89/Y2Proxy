  try{
	var classes = clova2.$(this).attr("class").split(" ");
	for(var i = 0; i < classes.length; i++){
		if(classes[i].indexOf("WTData:") == 0){
			var tokens = classes[i];
			tokens = tokens.split("(")[1].split(")")[0].split(",");
			for(var j = 0; j < tokens.length; j++){
				var tagname = tokens[j].split("=")[0];
				if(tagname == "WT.ac"){
					var tagvalue = tokens[j].split("=")[1];
				  utag.DB('Captured lead click: ' + tagvalue);
					setAnalyticsVariables({LinkValue: tagvalue}, false, "event");
				}
			}
		}
	}
}
  catch(e){
}
});
