function(a,b){
var hashCode = function(str) {
  var hash = 0, i, chr, len;
  if (str.length === 0) return hash;
  for (i = 0, len = str.length; i < len; i++) {
    chr   = str.charCodeAt(i);
    hash  = ((hash << 5) - hash) + chr;
    hash |= 0;
  }
  return (hash >>> 0).toString(16).toUpperCase();
};

b.UniqueEventID = hashCode(document.cookie).slice(-7);
var temp = "";
for(var k in b){
  temp += k + b[k];
}
b.UniqueEventID += hashCode(temp).slice(-7);
b.UniqueEventID += (new Date()).getTime().toString(16).toUpperCase().slice(-10)
},
