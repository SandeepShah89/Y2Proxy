c2.func.detectWebtrends = function(){
	var wtfound = false;
	c2.$("script").each(function(a){
		if(c2.$(this).attr("src") && c2.$(this).attr("src").indexOf("analyticsver10") >= 0) wtfound = true;
		if(c2.$(this).attr("src") && c2.$(this).attr("src").indexOf("analytics-min16") >= 0) wtfound = true;
		if(c2.$(this).attr("src") && c2.$(this).attr("src").indexOf("mobileanalytics") >= 0) wtfound = true;
		if(c2.$(this).attr("src") && c2.$(this).attr("src").indexOf("common/script/webtrends") >= 0) wtfound = true;
		if(c2.$(this).attr("src") && c2.$(this).attr("src").indexOf("assets/js/webtrends/webtrends") >= 0) wtfound = true;
	});
	if(window._tag) wtfound = true;
	if(window.Webtrends && window.Webtrends.version == "8.6.2") wtfound = true;
	return wtfound;
}

c2.func.deployWebtrends = function(){
	if(!c2.func.detectWebtrends()) {
		utag.DB("Webtrends base tag not found, adding..");
		utag.data.AddWebtrends = 1;
		c2.utag.AddWebtrends = 1;
		c2.capabilities.webtrends = true;
	} else {
		c2.capabilities.webtrends = false;
	}
}

c2.func.populateUDO = function(udo){
	for(var param in c2.utag){
		if(!udo[param] || udo[param] == ""){
			udo[param] = c2.utag[param];
		}
	}
}

}catch(e){};
if(!utag_condload){try{

var clovaAcquire = {
	config: {},

  loaded: 0,
	
	init: function(){
	  utag.DB("Initialising CLOVA Acquire");
	  if(clovaAcquire.loaded == 1) return;
	    clovaAcquire.loaded = 1;
		if(window.localAnalyticsConfig ) for(k in window.localAnalyticsConfig ) clovaAcquire.config[k] = window.localAnalyticsConfig[k] ;
		if(window.globalAnalyticsConfig) for(k in window.globalAnalyticsConfig) clovaAcquire.config[k] = window.globalAnalyticsConfig[k];
		
		/* This stuff is only going to work on the legacy platform, where the interface isn't dynamically rebuilt all the time */
		for(k in clovaAcquire.config){
			if(clovaAcquire.config[k]["listener"]){
				window.clova2.$("#" + k).attr("data-tealium-listener",clovaAcquire.config[k]["listener"]);
			}
		}
		
		clovaAcquire.attachListeners();
	},
	
	
	attachListeners: function(){
		window.clova2.$(document).on("blur", "input, select, [data-tealium-listener='blur']", 
			function(e){ clovaAcquire.analyticsEvent(this); }
		);
		window.clova2.$(document).on("click", "button, a[target], [data-tealium-listener='click']", 
			function(e){ clovaAcquire.analyticsEvent(this); }
		);
		window.clova2.$(document).on("mouseenter", "[data-tealium-listener='hover']", 
			function(e){ clovaAcquire.analyticsEvent(this); }
		);
		window.clova2.$(window).on("hashchange",
			function(){ clovaAcquire.hashEvent(window.location.hash.split("?")[0]); }
		);
	},

	
	analyticsEvent: function(t){
		var data = clovaAcquire.getAnalyticsData(t);
		/* Delay goes here */
		clovaAcquire.getRemoteData(data);
		console.log(data);
		if(data.persist) {
			var p = {};
			if(typeof data.persist == "function") data.persist = data.persist();
			if(typeof data.persist == "object"){
				p = data.persist;
				clovaAcquire.getRemoteData(p);
			} else {
				p[data.persist] = data.value;
			}
			clovaAcquire.setAnalyticsVariables(p,true,"none");
		}
		if(data.tracking != "full") delete data["value"];
		if(data.tracking != "none"){
			clovaAcquire.setAnalyticsVariables(data,false,"event");
		}
	},
	
	
	hashEvent: function(h, forceTrack){
	  	h = "#" + h.replace("#","");
		var data = { event: "Hash Change", narrative: h, tracking: "none"};
	  if(forceTrack) {
	    data.tracking = "default";
	    setAnalyticsVariables({JourneyStepName: h}, true);
	  }
		if(clovaAcquire.config["#"]){
			for(k in clovaAcquire.config["#"]){
				data[k] = clovaAcquire.config["#"][k];
			}
		}
		if(clovaAcquire.config[h]){
			for(k in clovaAcquire.config[h]){
				data[k] = clovaAcquire.config[h][k];
			}
		}
		
		/* Delay goes here */
		clovaAcquire.getRemoteData(data);
		console.log(data);
		if(data.persist) {
			var p = {};
			if(typeof data.persist == "function") data.persist = data.persist();
			if(typeof data.persist == "object"){
				p = data.persist;
				clovaAcquire.getRemoteData(p);
			} else {
				p[data.persist] = data.value;
			}
			clovaAcquire.setAnalyticsVariables(p,true,"none");
		}
		if(data.tracking != "full") delete data["value"];
		if(data.tracking != "none"){
			clovaAcquire.setAnalyticsVariables(data,false,"event");
		}
	},
	
	
	getId: function(t){
		return t.dataset.tealiumId || t.name || t.id;
	},
	
	
	getAnalyticsData: function(t){

		var id = clovaAcquire.getId(t);
		var defaultevent = "";
		var defaultaction = "";
		var defaultnarrative = "";
		var defaultvalue = "";

		switch(t.tagName.toLowerCase()){
			case "select":
			case "input":
			
				defaultevent = "Form Entry";
				defaultaction = "Field Update";
				defaultnarrative = t.name || t.id;
				defaultvalue = t.value || "";
			
				switch(t.type){
					case "submit":
						defaultevent = "Form Click";
						defaultaction = "Page Submit";
						defaultnarrative = t.value;
						break;
				
					case "reset":
					case "button":
						defaultevent = "Form Click";
						defaultaction = "Button Click";
						defaultnarrative = t.value;
						break;

					case "image":
						defaultevent = "Form Click";
						defaultaction = "Button Click";
						defaultnarrative = t.alt || t.src;
						break;
					
					case "checkbox":
						defaultvalue = t.checked;
						break;
				
					case "password":
						defaultvalue = "";
						break;
				
					default:
						break;
				}
		
				break;
		
			case "button":
				defaultevent = "Form Click";
				defaultaction = "Button Click";
				defaultnarrative = t.value || t.innerHTML.replace(/(<([^>]+)>)/ig,"");
				break;
			
			case "a":
				defaultevent = "External Click";
				defaultaction = "Link Click";
				defaultnarrative = t.title || t.innerHTML.replace(/(<([^>]+)>)/ig,"");
				break;
			
			default:
				break;
		}
	
		if(defaultaction == "Button Click"){
			if(["next","continue","confirm","submit"].indexOf(defaultnarrative.toLowerCase()) >= 0) defaultaction = "Page Submit";
			if(["previous","back","cancel"].indexOf(defaultnarrative.toLowerCase()) >= 0) defaultaction = "Page Cancel";
		}
	
		var data = {
			event: defaultevent,
			action: defaultaction,
			narrative: defaultnarrative,
			value: defaultvalue,
			tracking: "default",
			persist: null
		};
	
		for(k in t.dataset){
			if(k.indexOf("tealium") == 0){
				data[k.replace("tealium","").toLowerCase()] = t.dataset[k];
			}
		}
		
		for(k in clovaAcquire.config[id]) data[k] = clovaAcquire.config[id][k];
		
		return data;
	
	},
	
	
	getRemoteData: function(data){
		for(k in data){
			if(data[k] && typeof data[k] == "string" && data[k].split("$").length == 2 && data[k].split("$")[0] == ""){
				data[k] = clovaAcquire.getAnalyticsData(window.clova2.$("[data-tealium-id='" + data[k].split("$")[1] + "']")[0]).value;
			}
		}
	},
	
	
	setAnalyticsVariables: function(payload, persist, send){

		if(typeof persist == "boolean" && persist == true){
			for(var param in payload){
				if(window.utag_data) {
					window.utag_data[param] = payload[param];
				} else if(window.clova2 && window.clova2.utag){
					window.clova2.utag[param] = payload[param];
				}
			}
		}
	
		if(send == "view" || send == "event"){
		  var transmit = {JourneyEvent: "", EventAction: "", EventNarrative: "", EventValue: ""};
			var u = window.utag_data || utag.data;
			for(var param in u){
				if(typeof transmit[param] == "undefined") transmit[param] = u[param];
			}
			
			var mapping = { event: "JourneyEvent", action: "EventAction", narrative: "EventNarrative", value: "EventValue" };
					  
			for(var param in payload){
				if(mapping[param]){
					transmit[mapping[param]] = payload[param];
				} else {
					transmit[param] = payload[param];
				}
			}
		
			if(send == "view") utag.view(transmit);
			if(send == "event") utag.link(transmit);

		}
		
	}

};

window.setAnalyticsVariables = clovaAcquire.setAnalyticsVariables;
window.triggerAnalyticsPageEvent = clovaAcquire.hashEvent;

}catch(e){}};
if(!utag_condload){try{
function storageAvailable(type, storage, x) {
	try {
		storage = window[type];
		x = '__storage_test__';
		storage.setItem(x, x);
		storage.removeItem(x);
		return true;
	}
	catch(e) {
		return false;
	}
}

