var c2 = window.clova2;

c2.data.PageViewID = c2.func.generateId();
c2.data.fpc = document.cookie.replace(/(?:(?:^|.*;\s*)WT_FPC\s*\=\s*id\=([^;^:]*).*$)|^.*$/, "$1");
c2.data.customerNumber = c2.func.makeNumeric(c2.data.fpc);

for(var idomain = 0; idomain < c2.func.domainPatterns.length; idomain++) {
	var result = window.location.host.match(c2.func.domainPatterns[idomain].regex);
	if(result) {
		
		var dlArray = ["Brand","Environment","Division","Platform"];
		
		for(var iDL = 0; iDL < dlArray.length; iDL++){
			if(typeof c2.func.domainPatterns[idomain][dlArray[iDL]] == "number") c2.utag[dlArray[iDL]] = result[c2.func.domainPatterns[idomain][dlArray[iDL]]];
			if(typeof c2.func.domainPatterns[idomain][dlArray[iDL]] == "string") c2.utag[dlArray[iDL]] = c2.func.domainPatterns[idomain][dlArray[iDL]];
		}
		
		if(c2.utag.Brand) c2.utag.Brand = c2.utag.Brand.substr(0,1).toLowerCase();
		
		if(window.location.host.indexOf("business") >= 0 && c2.utag.Division == "p") c2.utag.Division = "b";
		
		var brandLookup = {
			"l" : "Lloyds",
			"b" : "BOS",
			"h" : "Halifax"
		};
		
		var divisionLookup = {
			"r" : "Retail",
			"p" : "Retail",
			"b" : "Business"
		};
		
		c2.utag.Brand = brandLookup[c2.utag.Brand];
		c2.utag.Division = divisionLookup[c2.utag.Division];
		
		break;
	}
}

