var ping = window.location && window.location.search && window.location.search.toLowerCase().indexOf("wt.mc_id") > 0 && window.location.href.toLowerCase().indexOf("bankofscotland.co.uk/aboutonline/") > 0;

var visibilityChecker = (function(){

  var initialState = currentState();
  var everVisible = initialState == "visible";

  var secondsVisible = 0;
  var secondsHidden = 0;

  function currentState(){
    try{
      if(document.visibilityState && document.visibilityState != "visible") return document.visibilityState;
      return (document.hasFocus && document.hasFocus()) ? "visible" : "hidden";
    } catch(e){ }
  }

  setInterval(function(){
    try{
      if(currentState() == "visible"){
      secondsVisible += 1;
      if(ping && window.setAnalyticsVariables && !everVisible){
        setAnalyticsVariables({JourneyEvent: "Ping", EventAmount: 0},false,"event");
      }
      if(ping && window.setAnalyticsVariables && secondsVisible == 30){
        setAnalyticsVariables({JourneyEvent: "Ping", EventAmount: 30},false,"event");
      }
      if(ping && window.setAnalyticsVariables && secondsVisible == 180){
        setAnalyticsVariables({JourneyEvent: "Ping", EventAmount: 180},false,"event");
      }
      everVisible = true;
    } else if (currentState() == "hidden"){
      secondsHidden += 1;
    }
  } catch(e){ }
  } , 1000);




  var exports = {};
  exports.getCurrentState = function(){ return currentState(); };
  exports.getInitialState = function(){ return initialState; };
  exports.getEverVisible = function(){ return everVisible; };
  exports.getSecondsVisible = function(){ return secondsVisible; };
  exports.getSecondsHidden = function(){ return secondsHidden; };
  return exports;
})();

try{
  var meta = document.createElement('meta');
  meta.name = "DCSext.pagevisibility";
  meta.content = visibilityChecker.getInitialState();
  document.getElementsByTagName('head')[0].appendChild(meta);
} catch(e) {}

}catch(e){}};
