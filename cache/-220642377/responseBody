function(a,b){
/* Check that the clova2 object exists and is ready to serve variables defining the brand etc. */
if(!window || !window.clova2 || !window.clova2.utag) throw "Data layer not found";

/* Platform lookup to identify test platforms and use a different UOID */
switch (window.clova2.utag.Platform || window.utag_data && window.utag_data.Platform){
  case "test":
    /* Override for LUAT */
    if(window.location.hostname.indexOf("luat") >= 0) {
      ATGSvcs.setUOID("200106303679");
    } else {
      ATGSvcs.setUOID("200106308269");
    }
    break;
  default:
    ATGSvcs.setUOID("200106303679");
}



ATGSvcs.setCfg("-eeSPA", 1);

var url = "";

/* Brand lookup to set correct domain, triggers an error if not a core brand */
switch (window.clova2.utag.Brand || window.utag_data && window.utag_data.Brand){
  case "Lloyds":
    url = window.clova2.utag.Platform == "test" ? "https://lloydsbank--tst.custhelp.com" : "https://help.lloydsbank.co.uk";
    break;
  case "Halifax":
    url = window.clova2.utag.Platform == "test" ? "https://halifax--tst.custhelp.com" : "https://help.halifax-online.co.uk";
    break;
  case "BOS":
    url = window.clova2.utag.Platform == "test" ? "https://bankofscotland--tst.custhelp.com" : "https://help.bankofscotland.co.uk";
    break;
  default:
    throw "Can't deploy Webchat, no brand found";
}

if(window.location.pathname.indexOf("/sharedealing/") === 0 || window.location.pathname.indexOf("/investments/") === 0 || window.location.pathname.indexOf("/share-dealing/") === 0){
  url = url.split("--tst").join("sharedealing--tst");
  if(url == "https://help.lloydsbank.co.uk") url = "https://help.lloydsbankdirectinvestmentsonline.co.uk";
  if(url == "https://help.halifax-online.co.uk") url = "https://help.halifaxsharedealing-online.co.uk";
  if(url == "https://help.bankofscotland.co.uk") url = "https://help.bankofscotlandsharedealing-online.co.uk";
}

if (u && !u.loaded) {
  u.loaded = true;
  u.loader({'src': url + "/rnt/rnw/javascript/vs/1/vsapi.js"});
  u.loader({'src': url + "/vs/1/vsopts.js"});
  console.log("Loading Webchat Resources");
}

var webChatContext = "WebChat";

if (b.JourneyName) {
  webChatContext += "-Journey-" + b.JourneyName;
} else if (b.ProductGroup) {
  webChatContext += "-Browse-" + b.ProductGroup;
  if (b.ProductSubGroup) {
    webChatContext += "-" + b.ProductSubGroup;
  }
} else {
  webChatContext += "-Unknown-" + window.location.hostname;
}

