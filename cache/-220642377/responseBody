if(!utag_condload){try{
window.clova2.func.setStorage = function(key, val, ttl){
	if(!window.localStorage) return;
	if(typeof val !== "object") val = { "_value" : val };
	val._expires = (new Date).getTime() + ((ttl || 60) * 1000);
	val._ttl = ttl || 60;
	try{
		window.localStorage.setItem(key,JSON.stringify(val));
	}
	catch(e){
	
	}
}

window.clova2.func.getStorage = function(key){
	if(!window.localStorage) return;
	try{
		var d = window.localStorage.getItem(key);
		if(d) d = JSON.parse(d);
		if(d && d._expires && d._expires >= (new Date).getTime()){
			delete d._expires;
			return d._value || d;
		} else {
			window.localStorage.removeItem(key);
		}
	}
	catch(e){
	
	}
	return;
}

window.clova2.func.modifyStorage = function(key, fn, fn2){
	var x = this.getStorage(key);
	if(x){
		fn(x);
		this.setStorage(key, x, x._ttl);
	} else {
		fn2(x);
	}
}

window.clova2.func.getDomain = function(fulldomain){
	if(fulldomain.split("//").length > 1){
		fulldomain = fulldomain.split("//")[1];
	}
	fulldomain = fulldomain.split("/")[0];
	var safedom = fulldomain.substr(fulldomain.length-3) == ".uk" ? fulldomain.substr(0,fulldomain.length-3) + "_uk" : fulldomain;
	if(safedom.split(".").length >= 2){
		return safedom.split(".")[safedom.split(".").length - 2] + "." + safedom.split(".")[safedom.split(".").length - 1].replace("_",".").split(":")[0];
	}
}

window.clova2.func.times = {};
window.clova2.func.times.seconds = 1;
window.clova2.func.times.minutes = clova2.func.times.seconds * 60;
window.clova2.func.times.hours = clova2.func.times.minutes * 60;
window.clova2.func.times.days = clova2.func.times.hours * 24;
window.clova2.func.times.weeks = clova2.func.times.days * 7;
window.clova2.func.times.months = clova2.func.times.days * 30;
window.clova2.func.times.quarters = clova2.func.times.days * 91;
window.clova2.func.times.years = clova2.func.times.days * 365;

window.loaderCallback = function(url, result){
  url = window.clova2.func.getDomain(url);
  clova2.func.modifyStorage(
    "pingresult",
    function(networkResults){
      if(!networkResults[url]) networkResults[url] = "";
      networkResults[url] += result ? "Y" : "N";
      if(networkResults[url].length > 10) networkResults[url] = networkResults[url].substr(networkResults[url].length-10,10);
    },
    function(){
      networkResults = {};
      networkResults[url] = result ? "Y" : "N";
      clova2.func.setStorage("pingresult", networkResults, clova2.func.times.months);
    }
  );
}
}catch(e){}};
