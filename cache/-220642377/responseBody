window.addMediaPixel = function(tracker, target, u){
	var deploy = 1;
	for(k in target){
		if(!target[k] || !u[k] || target[k] != u[k]){
			deploy = 0;
			break;
		}
	}
	if(deploy == 1){
	  console.log(tracker);
          window.addedMediaPixel = tracker;
	}
}
