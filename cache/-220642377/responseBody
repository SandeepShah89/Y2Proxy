	window.clova2.$ = jQuery.noConflict( true );
}
catch(e){
	if(window.utag && window.utag.DB) utag.DB(e);
	if(jQuery && !window.clova2.$) window.clova2.$ = jQuery;
}



c2.func.collect = function(){
  
  var mobileDCSIDs = {"dcsxby8tz00000w8oh3utaci2_8p2n":"Mobile","dcs33ei9u10000kby9iq3fci2_2x7f":"Mobile","dcs23qd9f00000k7z7ep4dci2_5y4p":"Mobile"};
  if(window._AP && window._AP.dcsid && mobileDCSIDs[window._AP.dcsid]) c2.utag.Platform = "mobile";
  if(window.utag_data && window.utag_data.Presentation == "Mobile") c2.utag.Platform = "mobile";
  
  utag.DB('Looking up DCSID for ' + (c2.utag.Brand ? c2.utag.Brand : "unknown brand") + ", on " + (c2.utag.Platform ? c2.utag.Platform : "unknown platform"));
  
  var thisBrand = (window.utag_data ? window.utag_data.Brand : undefined) || c2.utag.Brand;
  var thisPlatform = (window.utag_data ? window.utag_data.Platform : undefined) || c2.utag.Platform;
  
  if(c2.func.dcsids[thisBrand] && c2.func.dcsids[thisBrand][thisPlatform]) {
    c2.utag.DCSID = c2.func.dcsids[thisBrand][thisPlatform];
  } else if(c2.func.dcsids[thisBrand]){
    c2.utag.DCSID = c2.func.dcsids[thisBrand]["auth"];
  } else {
    c2.utag.DCSID = c2.func.dcsids["Lloyds"]["auth"];
  }
  
  if(c2.utag.Division == "Business"){
	if(c2.utag.Platform == "public" || c2.utag.Platform == "secure"){
	  c2.utag.DCSID = c2.func.dcsids[c2.utag.Brand]["business" + c2.utag.Platform];
	}
  }
  
  if(!c2.utag.DCSID) c2.utag.DCSID = c2.func.dcsids["Lloyds"]["auth"];
  
  utag.DB('Using DCSID ' + c2.utag.DCSID);
  
	try{
		if(window.pageAnalyticsElementArray){
			for(k in window.pageAnalyticsElementArray) {
				if(c2.func.dlparams[k]) {
					if(!c2.utag[c2.func.dlparams[k]]){
						c2.utag[c2.func.dlparams[k]] = window.pageAnalyticsElementArray[k]["tagValue"];
						if(c2.utag[c2.func.dlparams[k]].substr(0,1) == "["){	
							var token = c2.utag[c2.func.dlparams[k]].split(";")[0];
						  /* lazy fix, do it better */
							var strfind = token.match(/\[(.*)\]/)[1];
						  var strreplace = document.getElementById(strfind) ? document.getElementById(strfind).innerHTML : strfind + " not found";
							c2.utag[c2.func.dlparams[k]] = token.replace("[" + strfind + "]",strreplace);
						}
					}
				}
				if(k == "WT.ti") delete window.pageAnalyticsElementArray[k];
			}
		}
	}
	catch (e) {
		utag.DB(e);
	}
	
	try{
		c2.$("meta").each(function(a){
			var metaname = c2.$(this).attr("name");
			if(metaname){
				if(c2.func.dlparams[metaname]) {
					if(!c2.utag[c2.func.dlparams[metaname]]) c2.utag[c2.func.dlparams[metaname]] = c2.$(this).attr("content");
				} else if (metaname.indexOf("WT.") == 0 || metaname.indexOf("DCSext.") == 0 || metaname.indexOf("DCS.") == 0) {
					if(!c2.utag[metaname]) c2.utag[metaname] = c2.$(this).attr("content");
				}
			}
		});
	}
	catch (e) {
		if(window.utag && window.utag.DB) utag.DB(e);
	}

}

